<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.upenn.cis350.sfs_mobile"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.upenn.cis350.sfs_mobile.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="edu.upenn.cis350.sfs_mobile.LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.upenn.cis350.sfs_mobile.HomeScreen"
            android:label="@string/title_activity_home_screen" >
        </activity>
        <activity
            android:name="edu.upenn.cis350.sfs_mobile.MyAppointments"
            android:label="@string/title_activity_my_appointments" >
        </activity>
        <activity
            android:name="edu.upenn.cis350.sfs_mobile.DisplayImmunizations"
            android:label="@string/title_activity_display_immunizations"
            android:parentActivityName="edu.upenn.cis350.graphics.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.upenn.cis350.graphics.MainActivity" />
        </activity>
    </application>

</manifest>
